{
  "name": "s.h.i.e.l.d.",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test jest --detectOpenHandles --coverage --setupFiles dotenv-flow/config --silent",
    "dev": "NODE_ENV=development nodemon -r dotenv-flow/config ./src/index.js",
    "migration-generate": "npx sequelize-cli migration:generate --migrations-path ./db-migration",
    "migration-development": "NODE_ENV=development npx  sequelize-cli db:migrate",
    "migration-development-undo": "NODE_ENV=development npx  sequelize-cli db:migrate:undo",
    "seeds-generate": "npx sequelize-cli seed:generate --seeders-path ./db-migration/seeders",
    "seeds-development": "NODE_ENV=development npx sequelize-cli db:seed",
    "seeds-development-undo": "NODE_ENV=development npx sequelize-cli db:seed:undo",
    "lint": "npx eslint \"src/**/*.js\"",
    "lint-fix": "npx eslint \"src/**/*.js\" --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv-flow": "^4.1.0",
    "express": "^4.19.2",
    "express-jsdoc-swagger": "^1.8.0",
    "express-validator": "^7.1.0",
    "jest": "^29.7.0",
    "mysql2": "^3.10.1",
    "sequelize": "^6.37.3",
    "supertest": "^7.0.0"
  },
  "engines": {
    "node": ">= 20"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "nodemon": "^3.1.3"
  }
}
